.iconbox{
    display: flex;
    padding-top: 10px;
    position: relative;
    z-index: 9999;
}
.iconbox .ge{
    flex:1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.iconbox .ge .pic{
    width: 60px;
    height:60px;
}
.iconbox .ge .word{
    font-size:14px;
}
.iconbox .ge .pic image{
    width: 100%;
    height: 100%;
}
.iconbox2{
    display: flex;
    position: relative;
    z-index: 9999;
}
.iconbox2 .ge{
    width: 150rpx;
    height: 150rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.iconbox2 .ge .pic{
    width: 100rpx;
    height: 100rpx;
}
.iconbox2 .ge .pic image{
    width: 100%;
    height: 100%;
}
.iconbox2 .ge .word{
    font-size: 14px;
}
.mask{
    position: absolute;
    top:0;
    left:0;
    width:100%;
    height: 130px;
    background: -webkit-linear-gradient(top,white,gold  20%);
    border-radius: 0 0 30% 30%;
    box-shadow: 0px 0px 1px rgba(0,0,0,.4);
}
.swiper{
    width: 680rpx;
    height: 219.3rpx;
    margin: 20px auto;
    position: relative;
    z-index: 9999;
}
.swiper .swiper-item{
    width: 680rpx;
    height: 219.3rpx;
}
.swiper .swiper-item image{
    width: 680rpx;
    height: 219.3rpx;
}